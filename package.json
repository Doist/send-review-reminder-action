{
  "name": "send-review-reminder-action",
  "version": "0.0.0",
  "private": false,
  "description": "Reminds about pending GitHub pull request review on twist.com",
  "prettier": "@doist/prettier-config",
  "scripts": {
    "build": "tsc",
    "check": "npm run eslint && npm run prettier:check && npm run build",
    "eslint": "eslint --format codeframe --ext js,ts ./",
    "package": "ncc build src/main.ts",
    "prettier:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,css,less,yml,yaml,md}\"",
    "prettier:format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,less,yml,yaml,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@doist/eslint-config": "7.1.2",
    "@doist/prettier-config": "3.0.5",
    "@doist/tsconfig": "1.0.0",
    "@types/node": "17.0.41",
    "@typescript-eslint/parser": "5.27.1",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.17.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-plugin-github": "4.3.6",
    "js-yaml": "4.1.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  }
}
